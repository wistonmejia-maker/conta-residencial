{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://conta-residencial-production.up.railway.app/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "crons": [
        {
            "path": "/api/scan/cron/scan-all",
            "schedule": "0 10 * * *"
        }
    ]
}